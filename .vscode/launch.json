    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart"
        },
        {
            "name": "Debug - RDT",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": ["--dart-define=RDT=true"]
        },
        {
            "name": "Debug - RDT/Local",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": ["--dart-define=RDT=true", "--dart-define=FIREBASE_EMULATORS=true"]
        },
        {
            "name": "Debug - RDT/Mocks",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": ["--dart-define=RDT=true", "--dart-define=MOCKS=true"]
        }
    ]
}